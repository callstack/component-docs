{
  "name": "component-docs",
  "version": "0.22.0",
  "description": "Documentation for React components",
  "bin": "bin/component-docs.js",
  "main": "dist/index.js",
  "files": [
    "bin/",
    "dist/",
    "components.js"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/satya164/component-docs.git"
  },
  "scripts": {
    "build": "babel src/ --source-maps --out-dir dist/ --copy-files && linaria -o dist/css -r src -i dist src/**/*.js",
    "lint:js": "eslint .",
    "lint:css": "stylelint src/**/*.js",
    "lint": "yarn lint:js && yarn lint:css",
    "flow": "flow",
    "prepare": "yarn build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "example": "babel-node src/cli.js serve --root example",
    "prebuild": "del dist/",
    "preexample": "del example/dist && yarn build",
    "release": "release-it"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint && yarn flow"
    }
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
